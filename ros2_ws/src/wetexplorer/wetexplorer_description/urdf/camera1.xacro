      </geometry>
    </collision> -->
  </link>
  <joint
    name="camera1_joint"
    type="fixed">
    <origin
      xyz="0.76557 0.117 0.455898357942271"
      rpy="0 1.3963 0" />
    <parent
      link="base_link" />
    <child
      link="camera1_link" />
    <axis
      xyz="0 0 0" />
  </joint>

  <link name="camera1_link_output">      
    </link>


  <joint name="camera1_joint_output"
      type="fixed">
      <origin
        xyz="0.0 0.0 0.0"
        rpy="-1.5708 0 -1.5708 " />
      <parent
        link="camera1_link" />
      <child
        link="camera1_link_output" />
      <axis
        xyz="0 0 0" />
  </joint>


  <link name="camera1_calib">      
    </link>


  <joint name="camera1_calib_joint"
      type="fixed">
      <origin
        xyz="0.768379569054 0.148829653859 0.454491734505"
        rpy="0 0 0 " />
      <parent
        link="base_link" />
      <child
        link="camera1_calib" />
      <axis
        xyz="0 0 0" />
  </joint>
  <link name="camera_link">      
    </link>
  <joint name="orbbec_joint"
      type="fixed">
      <origin
        xyz="0.0 0.023878177 0.0"
        rpy="0 1.3963 0" />
      <parent
        link="camera1_calib" />
      <child
        link="camera_link" />
      <axis
        xyz="0 0 0" />
  </joint>




  <gazebo reference="camera1_link">
      <material>Gazebo/Black</material>

      <sensor name="depth_camera1" type="depth">
          <pose> 0 0 0 0 0 0 </pose>
          <visualize>true</visualize>
          <update_rate>15</update_rate>
          <camera>
              <camera_info_topic>camera1/camera_info</camera_info_topic>
              <horizontal_fov>1.518</horizontal_fov>
              <image>
                  <format>R8G8B8</format>
                  <width>640</width>
                  <height>480</height>
              </image>
              <clip>
                  <near>0.1</near>
                  <far>5.0</far>
              </clip>
          </camera>
          <topic>camera1/depth_raw</topic>
          <gz_frame_id>camera1_link_output</gz_frame_id>
      </sensor>
      <sensor name="visual_camera1" type="camera">
          <pose> 0 0 0 0 0 0 </pose>
          <visualize>true</visualize>
          <update_rate>15</update_rate>
          <camera>
              <camera_info_topic>camera1/camera_info</camera_info_topic>
              <horizontal_fov>1.518</horizontal_fov>
              <image>
                  <format>R8G8B8</format>
                  <width>640</width>
                  <height>480</height>
              </image>
              <clip>
                  <near>0.3</near>
                  <far>5.0</far>
              </clip>
          </camera>
          <topic>camera1/image_raw</topic>
          <gz_frame_id>camera1_link_output</gz_frame_id>
      </sensor>
  </gazebo>

