<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="camera">

  
  <link
    name="camera1_link">
    <inertial>
      <origin
        xyz="-0.0131182411372806 0.000373990072271901 -0.000195973766208279"
        rpy="0 0 0" />
      <mass
        value="0.0460883337703012" />
      <inertia
        ixx="5.83719963366224E-05"
        ixy="2.47510070128998E-07"
        ixz="-1.0827158801375E-07"
